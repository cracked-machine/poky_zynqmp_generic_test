  /_w/poky_zynqmp_generic_test/poky_zynqmp_generic_test/layers/meta-xilinx/meta-xilinx-bsp \
  /_w/poky_zynqmp_generic_test/poky_zynqmp_generic_test/layers/meta-xilinx/meta-xilinx-standalone \  
  /_w/poky_zynqmp_generic_test/poky_zynqmp_generic_test/layers/meta-xilinx-tools \
  /_w/poky_zynqmp_generic_test/poky_zynqmp_generic_test/layers/poky/meta-poky \
  /_w/poky_zynqmp_generic_test/poky_zynqmp_generic_test/layers/poky/meta-yocto-bsp \
  /_w/poky_zynqmp_generic_test/poky_zynqmp_generic_test/layers/meta-test \
